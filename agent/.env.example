# ===========================================
# Voice Assistant Agent - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# LiveKit Configuration
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# Deepgram (STT & TTS) - $200 free credit
# Get your key at: https://deepgram.com
DEEPGRAM_API_KEY=your_deepgram_api_key

# Google Gemini (LLM) - $300 free credit
# Get your key at: https://ai.google.dev
GOOGLE_API_KEY=your_google_api_key

# Mem0 (Memory)
# Get your key at: https://mem0.ai
MEM0_API_KEY=your_mem0_api_key

# Qdrant (Vector Store)
# Get your cluster at: https://cloud.qdrant.io
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key

# Supabase (for user data sync)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_service_role_key

# Optional: Tool APIs
SERPAPI_API_KEY=your_serpapi_key
OPENWEATHER_API_KEY=your_openweather_key
